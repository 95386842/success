<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title><%=title%></title>
</head>
<body>
<div id="photos">
    <% photos.forEach(function(photo) { %>
    <div class="photo">
        <h2><%=photo.name%></h2>
        <a href='/photo/<%=photo.id%>/download'>
            <img src='<%=photo.path%>'/>
        </a>
    </div>
    <% }) %>
</div>
</body>
</html>